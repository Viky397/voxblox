<?xml version="1.0"?>
<!-- Author: Joe Qian -->
<launch>
    <arg name="revis" default="1" />
    <arg name="simulation" default="true" />
    <param name="/use_sim_time" value="$(arg simulation)"/>
    

    <!-- Launch Object Detection and Traffic Light Detection -->
    <include file="$(find object_detection)/launch/perception_cpu.launch">
    </include>
    
    <!-- Launch  Local Visualization Republisher -->
    <include file="$(find custom_vis)/launch/revis.launch" if="$(arg revis)"/>
    
</launch>
